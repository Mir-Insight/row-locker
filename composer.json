{
    "name": "mir-insight/row-locker",
    "description": "Plugin to lock database rows using the CakePHP ORM",
    "type": "cakephp-plugin",
    "license": "MIT",
    "version": "1.0.1",
    "require": {
        "php": "^8.1",
        "cakephp/orm": "^4.0|^5.0"
    },
    "require-dev": {
        "phpunit/phpunit": "*",
        "cakephp/cakephp": "^4.0|^5.0"
    },
    "autoload": {
        "psr-4": {
            "RowLocker\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "RowLocker\\Test\\": "tests",
            "Cake\\Test\\": "./vendor/cakephp/cakephp/tests"
        }
    }
}
